{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/gclient" ],
			"stopOnEntry": false,
			"sourceMaps": true,
			"outFiles": [ "${workspaceRoot}/gclient/out/src/**/*.js" ],
			"preLaunchTask": "npm"
		}
	]
}